@model IEnumerable<SchoolBaseObject>


<table class="table table-striped table-hover table-sm">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Full Name</th>
            <th scope="col">Unique Id</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <th scope="row">1</th>
                <td>@obj.Name</td>
                <td>@obj.Id</td>
            </tr>
        }
  </tbody>
</table>